{
  "expo": {
    "name": "Pokedex",
    "slug": "pokedex",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A basic Gen-1 Pokedex app written in React Native with Apollo Client to practice GraphQL. Other than that this app doesn't do much really.",
    "githubUrl": "https://github.com/kelokchan/pokedex-react-apollo"
  }
}